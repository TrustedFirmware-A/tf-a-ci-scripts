pre_tf_build() {
    local ext_lib="libqtisec.a"

    pushd "${workspace}"

    if [ ! -f "${ext_lib}" ]; then
        echo "Fetch ${ext_lib} for RB3Gen2"
        saveas="${ext_lib}" \
            url="https://github.com/coreboot/qc_blobs/raw/master/sc7280/qtiseclib/libqtisec.a" \
            fetch_file
        pwd; ls -l
    fi

    popd "${workspace}"
}
