source [file join [file dirname [info script]] handle-arguments.inc]

# Expect an error while loading BL2 image.
expect {
	"Loading of FW_CONFIG failed" { exit_uart 0 }
	"BL1: Booting BL2"		{ exit_uart -1 }
	timeout				{ exit_timeout }
}
