generate_lava_job() {
	local model="base-aemv8a"

	uart="0" set_expect_variable "num_cpus" "1"

	model="$model" \
		cluster_0_num_cores="1" \
		cluster_1_num_cores="0" \
		gen_model_params

	model="$model" gen_fvp_yaml
}
