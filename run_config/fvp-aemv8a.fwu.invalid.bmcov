generate_lava_job() {
	local model="base-aemv8a"

	model="$model" \
		bmcov_plugin="1" \
		uart0_shutdown_tag="WARNING: Image id=1 size out of bounds" \
		bmcov_plugin_path="${coverage_trace_plugin}" \
		gen_model_params

	model="$model" gen_fvp_yaml
}
