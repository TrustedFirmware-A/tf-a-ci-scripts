post_tf_build() {
	# Build FIP without any BL33 image (it is preloaded)
	build_fip
}

post_fetch_tf_resource() {
	preload_bl33="1" \
	preload_bl33_bin="tftf.bin" \
		model="base-aemv8a" gen_model_params

	model="base-aemv8a" \
	model_bin="FVP_Base_AEMv8A-AEMv8A" \
	       gen_fvp_yaml
}
