pre_tf_build() {
	# Apply ubsan test patch
	apply_tf_patch "ubsan/ubsan_test.patch"
}

post_tf_archive() {
	model="base-aemv8a" \
		gen_model_params

	model="base-aemv8a" \
	model_bin="FVP_Base_AEMv8A-AEMv8A" \
	       gen_fvp_yaml
}

fetch_tf_resource() {
	# Expect trap message on uart 1
	uart="1" set_primary="1" file="ubsan-test-trap.exp" track_expect
}
