fetch_tf_resource() {
	kernel_type="fvp-oe-uboot" get_kernel
	initrd_type="dummy-ramdisk" get_initrd

	tmp_dir="$(mktempdir)"
	ln -s "${img_file:?}" "$archive/rootfs.bin"
}

