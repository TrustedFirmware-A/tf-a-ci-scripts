generate_lava_job() {
	local model="neoverse_e1"

	uart="0" set_expect_variable "num_cpus" "2"

	model="$model" \
	cluster_0_num_cores="1" \
	cache_state_modelled="1" \
	data_instance="cluster0.cpu0.thread0" \
	gen_model_params

	model="$model" gen_fvp_yaml
}
