generate_lava_job() {
	local model="cortex-a710"

	uart="0" set_expect_variable "num_cpus" "4"

	model="$model" cache_state_modelled="0" cluster_0_num_cores="4" \
		gen_model_params
	model="$model" gen_fvp_yaml
}
