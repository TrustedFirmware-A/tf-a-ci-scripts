generate_lava_job() {
	local model="base-aemv8a-crypto"

	model="$model" \
	arch_version="8.2" \
	has_ras="1" \
	has_sha3="2" \
	has_aes="1" \
	has_sha1="1" \
	has_sha256="1" \
	has_sha512="2" \
	crypto_plugin="1" \
	crypto_plugin_path="${crypto_plugin_path}" \
	gen_model_params

	model="$model" gen_fvp_yaml
}