fetch_tf_resource() {
	kernel_type="fvp-aarch32-zimage" get_kernel
	initrd_type="aarch32-ramdisk" get_initrd
	uart="0" file="linux-rd-busybox-aarch32.exp" track_expect
	uart="0" set_expect_variable "skip_early_boot_msgs" "1"
}
