post_tf_build() {
	get_uboot_bin
}

post_fetch_tf_resource() {
	image="$(mktempfile)"
	printf '\1\0\0\0' > "$image"

	preload_bl33="1" \
	preload_bl33_bin="uboot.bin" \
	reset_to_bl31="1" \
	cache_state_modelled="0" \
	memprotect="$image" \
	memprotect_addr="0xBFC0000" \
		model="base-aemv8a" gen_model_params
}
